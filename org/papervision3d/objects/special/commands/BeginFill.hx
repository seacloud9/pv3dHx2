package org.papervision3d.objects.special.commands {	import org.papervision3d.objects.special.commands.IVectorShape;	import flash.display.Graphics;			/**	 * @author Mark Barcinski	 */	class BeginFill implements IVectorShape {		public var fillColor:Int;		public var fillAlpha:Float;					public function new(fillColor:Int=0x000000, fillAlpha:Float=1):Void		{			this.fillColor=fillColor;			this.fillAlpha=fillAlpha;		}		public function draw(graphics:Graphics , prevDrawn:Bool):Bool {			graphics.beginFill(fillColor , fillAlpha);						return true;		}	}